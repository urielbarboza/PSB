 "cells": [
  {
   "cell_type": "code",
   "execution_count": 1,
   "metadata": {},
   "outputs": [],
   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "from scipy.integrate import odeint\n",
    "from scipy.optimize import minimize\n",
    "import matplotlib.cm as cm"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 2,
   "metadata": {},
   "outputs": [],
   "source": [
    "# define model\n",
    "def Amodel(y, t, k):\n",
    "    dydt= np.empty(1)\n",
    "    dydt[0]= -k*y[0]\n",
    "    return dydt"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# generate some data\n",
    "nd= 40\n",
    "ke= 2.3\n",
    "A0e= 1000\n",
    "sig= 200\n",
    "t= np.linspace(0, 1, nd)\n",
    "y= odeint(Amodel, [A0e], t, args= (ke,))\n",
    "d= y[:,0] + np.random.randn(np.size(y[:,0]))*sig\n",
    "\n",
    "# plot data\n",
    "plt.figure()\n",
    "plt.plot(t, d, 'r.', markersize= 10)\n",
    "plt.xlabel('time')\n",
    "plt.ylabel('A')\n",
    "plt.xlim([-0.05, 1.05])\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 4,
   "metadata": {},
   "outputs": [],
   "source": [
    "# define energy (- log likelihood)\n",
    "def energy(p, t, d, sig):\n",
    "    k, A0= p\n",
    "    n= len(d)\n",
    "    y= odeint(Amodel, [A0], t, args= (k,))\n",
    "    e= np.sum((d - y[:,0])**2/2/sig**2 + np.log(sig) + 0.5*np.log(2*np.pi))\n",
    "    return e"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "metadata": {},
   "outputs": [],
   "source": [
    "# find energy surface\n",
    "gridsize= 100\n",
    "a0s= np.linspace(600, 2000, gridsize)\n",
    "ks= np.linspace(1, 10, gridsize)\n",
    "e= np.empty((gridsize, gridsize))\n",
    "for i in np.arange(gridsize):\n",
    "    for j in np.arange(gridsize):\n",
    "        e[i,j]= energy([ks[j], a0s[i]], t, d, sig)"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 648x648 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# plot energy surface (posterior)\n",
    "plt.figure(figsize=(9,9))\n",
    "plt.contourf(ks, a0s, -e, 25, origin= 'lower', cmap= cm.gray)\n",
    "plt.plot(ke, A0e, 'rd', markersize= 10)\n",
    "plt.title('sigma= {:4.2f} ; nd= {:d}'.format(sig, nd))\n",
    "plt.xlabel('k')\n",
    "plt.ylabel('A0')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 7,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 648x648 with 1 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# find minimum of energy\n",
    "res= minimize(energy, [5, 450], args= (t, d, sig))\n",
    "\n",
    "# plot energy surface (posterior)\n",
    "plt.figure(figsize=(9,9))\n",
    "plt.contourf(ks, a0s, -e, 25, origin= 'lower', cmap= cm.gray)\n",
    "plt.plot(ke, A0e, 'rd', markersize= 10)\n",
    "plt.plot(res.x[0], res.x[1], 'bo', markersize= 10)\n",
    "plt.title('sigma= {:4.2f} ; nd= {:d}'.format(sig, nd))\n",
    "plt.xlabel('k')\n",
    "plt.ylabel('A0')\n",
    "plt.show()"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 8,
   "metadata": {},
   "outputs": [
    {
     "data": {
      "image/png": "\n",
      "text/plain": [
       "<Figure size 432x288 with 2 Axes>"
      ]
     },
     "metadata": {
      "needs_background": "light"
     },
     "output_type": "display_data"
    }
   ],
   "source": [
    "# calculate marginal probabilites\n",
    "p= np.exp(-e + np.min(e))\n",
    "pk= np.sum(p,0)/np.sum(p)\n",
    "pa0= np.sum(p,1)/np.sum(p)\n",
    "\n",
    "# calculate best estimates\n",
    "kb= ks[np.argmax(pk)]\n",
    "kberr= np.sqrt(np.sum((ks - kb)**2*pk))\n",
    "a0b= a0s[np.argmax(pa0)]\n",
    "a0berr= np.sqrt(np.sum((a0s - a0b)**2*pa0))\n",
    "\n",
    "# plot marginals\n",
    "plt.figure()\n",
    "plt.subplot(2,1,1)\n",
    "plt.plot(ks, pk)\n",
    "plt.xlim([np.min(ks), np.max(ks)])\n",
    "plt.errorbar(kb, np.max(pk)/2, xerr= kberr, color= 'k', fmt= 'o')\n",
    "plt.fill_between(ks, 0, pk, alpha= 0.3)\n",
    "plt.ylabel('probability')\n",
    "plt.xlabel('k')\n",
    "plt.subplot(2,1,2)\n",
    "plt.plot(a0s, pa0)\n",
    "plt.xlim([np.min(a0s), np.max(a0s)])\n",
    "plt.errorbar(a0b, np.max(pa0)/2, xerr= a0berr, color= 'k', fmt= 'o')\n",
    "plt.fill_between(a0s, 0, pa0, alpha= 0.3)\n",
    "plt.ylabel('probability')\n",
    "plt.xlabel('A0')\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
  },
